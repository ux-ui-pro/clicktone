{
  "name": "clicktone",
  "version": "1.1.0",
  "description": "A simple class that provides a convenient abstraction for handling audio playback by web applications, with support for iOS devices.",
  "author": "ux-ui.pro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ux-ui-pro/clicktone.git"
  },
  "bugs": {
    "url": "https://github.com/ux-ui-pro/clicktone/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "targets": {
    "main": {
      "source": "src/index.js"
    },
    "module": {
      "source": "src/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "build": "yarn clean && parcel build",
    "lint:js": "eslint --ext .js",
    "lintfix": "yarn lint:js --fix"
  },
  "devDependencies": {
    "eslint": "^7.32.0 || ^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "parcel": "^2.11.0",
    "postcss-html": "^1.6.0"
  },
  "keywords": [
    "MP3",
    "WAV",
    "OGG",
    "AAC",
    "iOS",
    "sound",
    "audio",
    "playback",
    "mobile"
  ]
}
